{
    "name": "spe/youtube",
    "description": "SPE Chapter 10 - YouTube Manager with PHP 8.5 features",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Constable",
            "email": "mc@netserva.org"
        }
    ],
    "require": {
        "php": ">=8.5",
        "google/apiclient": "^2.15"
    },
    "autoload": {
        "psr-4": {
            "SPE\\YouTube\\": "src/"
        }
    },
    "scripts": {
        "post-update-cmd": "Google\\Task\\Composer::cleanup"
    },
    "extra": {
        "google/apiclient-services": ["YouTube"]
    },
    "config": {
        "optimize-autoloader": true
    }
}
